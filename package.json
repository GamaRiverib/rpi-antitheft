{
  "name": "rats",
  "version": "0.1.0",
  "description": "AntiTheft System Raspberry Pi 3 Based using Node.JS that expose the web services interface with REST",
  "main": "dist/index.js",
  "private": true,
  "engines": {
    "node": "8.9.4",
    "npm": "5.6.0"
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "tsc --watch & nodemon dist --ignore 'Config.json'",
    "test": "set NODE_ENV=test&& set NODE_PORT=3000&& tsc&& mocha dist/**/*.spec.js --reporter spec",
    "coverage": "istanbul cover node_modules/mocha/bin/_mocha -- -R spec",
    "lint": "eslint src --ext ts",
    "tsc": "tsc"
  },
  "keywords": [
    "AntiTheft",
    "Alarm"
  ],
  "author": {
    "name": "Jos√© Gamaliel Rivera Ibarra",
    "email": "gama.rivera@gmail.com"
  },
  "license": "ISC",
  "dependencies": {
    "bleno": "^0.5.0",
    "bunyan": "1.8.12",
    "bunyan-winston-adapter": "0.2.0",
    "jssha": "^2.3.1",
    "nodemailer": "^5.1.1",
    "onoff": "^3.0.2",
    "q": "1.5.1",
    "restify": "7.1.1",
    "serialport": "^6.2.0",
    "socket.io": "^2.1.0",
    "winston": "3.1.0",
    "winston-daily-rotate-file": "3.5.1"
  },
  "devDependencies": {
    "@types/bleno": "^0.4.0",
    "@types/chai": "4.1.3",
    "@types/jssha": "0.0.29",
    "@types/mocha": "5.2.0",
    "@types/node": "9.6.8",
    "@types/onoff": "0.0.27",
    "@types/restify": "^5.0.8",
    "@types/serialport": "^6.0.1",
    "@types/socket.io": "^1.4.33",
    "chai": "4.1.2",
    "chai-http": "4.0.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-promise": "3.7.0",
    "istanbul": "0.4.5",
    "mocha": "5.1.1",
    "mocha-typescript": "1.1.12",
    "nodemon": "^1.17.5",
    "supertest": "3.0.0",
    "tsc": "1.20150623.0",
    "typescript": "2.8.3",
